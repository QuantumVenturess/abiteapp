// Insert data into page
$('.userShowTables').append('<%= j render partial: "users/user_table", 
  collection: @tables, as: :table %>');
// Remove and add pagination
<% if @tables.num_pages == @tables.current_page %>
  $('.page').remove();
<% else %>
  $('.page').html('<%= j render partial: "shared/pagination", 
    object: @tables, as: :objects %>');
<% end %>
// Lazy load
$('.lazyLoad').each(function() {
  var original = $(this).attr('data-original');
  $(this).attr('src', original);
});
// Links
$('.link').click(function() {
  if ($(this).attr('href')) {
    window.location = $(this).attr('href');
  };
});
// Spinner
$('.spinner').hide();