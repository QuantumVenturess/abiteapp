// Insert data into page
$('.userShow .complete').append('<%= j render partial: "users/complete_tables", 
  object: @seats, as: :seats %>');
// Remove and add pagination
<% if @seats.num_pages == @seats.current_page %>
  $('.page').remove();
<% else %>
  $('.page').html('<%= j render partial: "shared/pagination", 
    object: @seats, as: :objects %>');
<% end %>
// Lazy load
$('.lazyLoad').each(function() {
  var original = $(this).attr('data-original');
  $(this).attr('src', original);
});
// Links
$('.link').click(function() {
  if ($(this).attr('href')) {
    window.location = $(this).attr('href');
  };
});
// Spinner
$('.spinner').hide();