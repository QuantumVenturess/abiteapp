<% if @destroy %>
  window.location = "<%= explore_path %>";
<% else %>
  // Insert join table form
  $('.joinTable').html('<%= j render partial: "tables/join_table", 
    object: @table, locals: { table: @table } %>');
  // Remove user's seat
  $('#userSeat_' + <%= current_user.id %>).remove();
  // Remove leave form
  $('.leave_table form').remove();
  // Remove message room button
  $('.table_show .messageRoomButton a').remove();
  // Remove compose icon
  $('.rightBarButtonItem').html('');
  // Update People sitting count
  $('.seats h1 span').text(
    '<%= @table.seats.size %> out of <%= @table.max_seats %>');
  // Remove messsages
  $('.table_show .messages').html('');
<% end %>