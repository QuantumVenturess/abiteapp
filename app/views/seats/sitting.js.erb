// Insert data into page
$('.ready .tables').append('<%= j render partial: "seats/sitting_table", 
  object: @seats_ready, as: :seats %>');
$('.waiting .tables').append('<%= j render partial: "seats/sitting_table", 
  object: @seats_waiting, as: :seats %>');
// Remove and add pagination
<% if @seats_ready.num_pages == @seats_ready.current_page || @seats_waiting.num_pages == @seats_waiting.current_page %>
  $('.page').remove();
<% else %>
  <% if @ready.size > @waiting.size %>
    $('.page').html('<%= render partial: "shared/pagination", object: 
      @seats_ready, as: :objects %>');
  <% else %>
    $('.page').html('<%= render partial: "shared/pagination", object: 
      @seats_waiting, as: :objects %>');
  <% end %>
<% end %>
// Lazy load
$('.lazyLoad').each(function() {
  var original = $(this).attr('data-original');
  $(this).attr('src', original);
});
// Links
$('.link').click(function() {
  if ($(this).attr('href')) {
    window.location = $(this).attr('href');
  };
});
// Spinner
$('.spinner').hide();